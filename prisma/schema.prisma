generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url = env("DATABASE_URL")
}

model hanky {
  id          Int       @id @default(autoincrement())
  content     String
  created     DateTime  @default(now())
  updated     DateTime  @updatedAt
}

model user {
  email     String      @id
  name      String
  pic       String
  created   DateTime    @default(now())
  updated   DateTime    @updatedAt
}
